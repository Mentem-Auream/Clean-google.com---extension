{
    "manifest_version": 3,
    "name": "Google Cleaner",
    "version": "1.1",
    "description": "Removes unwanted elements from Google's homepage",
    "permissions": ["scripting"],
    "host_permissions": ["https://www.google.com/"],
    "content_scripts": [
      {
        "matches": ["https://www.google.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  